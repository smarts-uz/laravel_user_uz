{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0|8.1",
        "ext-json": "*",
        "babenkoivan/elastic-scout-driver": "^3.0",
        "beyondcode/laravel-websockets": "^1.13",
        "codeception/module-db": "*",
        "codeception/module-rest": "*",
        "darkaonline/l5-swagger": "^8.3",
        "fruitcake/laravel-cors": "^2.0",
        "glhd/aire": "^2.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "joy/voyager-api": "^1.2",
        "laravel/framework": "^8.78",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.4",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "munafio/chatify": "^1.3",
        "napaeduhub/eskiz-sms-client": "^1.0",
        "realrashid/sweet-alert": "^4.1",
        "tcg/voyager": "^1.5",
        "teamprodev/laravel-payment": "^1.0",
        "teamprodev/playmobile-sms": "^1.0",
        "temprodev/laravel-one-auth": "dev-main",
        "ua-parser/uap-php": "^3.9",
        "vemcogroup/laravel-translation": "^3.0",
        "yajra/laravel-datatables": "1.5",
        "yajra/laravel-datatables-buttons": "^4.13",
        "yajra/laravel-datatables-oracle": "^9.20"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "codeception/codeception": "^4.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nikaia/translation-sheet": "^1.5",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PayUz\\": "app/Services/PaynetNew/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
